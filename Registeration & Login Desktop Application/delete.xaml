<Page x:Class="Registeration___Login_Desktop_Application.delete"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Registeration___Login_Desktop_Application"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="delete">

    <Grid>
        <Grid Background="LightBlue">
            <TextBox x:Name="tx" HorizontalAlignment="Center" Margin="0,68,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="244" Height="30"/>
            <Button x:Name="del" Content="delete" HorizontalAlignment="Left" Margin="622,73,0,0" VerticalAlignment="Top" FontSize="16" FontWeight="Bold" FontStyle="Italic" Click="del_Click"/>
            <DataGrid Name="dg" d:ItemsSource="{d:SampleData ItemCount=0}" Margin="0,174,0,10" SelectionChanged="dg_SelectionChanged"/>
        </Grid>
        <Label x:Name="phon" Content="phone numbr" HorizontalAlignment="Left" Margin="94,58,0,0" VerticalAlignment="Top" FontSize="20" FontStyle="Italic" FontWeight="Bold"/>
        <Label x:Name="d" Content="delete page" HorizontalAlignment="Center" Margin="0,10,0,0" VerticalAlignment="Top" FontSize="20" FontWeight="Bold" FontStyle="Italic"/>


    </Grid>
</Page>
